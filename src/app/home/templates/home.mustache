{{>header}}

<main>
    <!-- START: template component(s) -->
    <div class="container" id="section-1">
        <div class="row">
            <div class="col-12">
            <h1>Mijn Nijmegen</h1>
                <p class="lead">Bekijk uw persoonsgegevens of uitkeringsgegevens.</p>
            </div>
        </div>
        <div class="row">
            {{#nav}}
            <div class="col-lg-4">
                <div class="card card-services">
                    <div class="card-body">
                        <p>{{{icon}}}
                        <h2 class="card-title">{{title}}</h2>
                        <p class="card-text">{{description}}</p>
                        <p class="card-read-more text-right mb-0 text-uppercase">
                            <a href="{{{url}}}" aria-label="{{label}}">Lees meer</a>
                        </p>
                    </div>
                </div>
            </div>
            {{/nav}}
        </div>
    </div>

    <!-- END: template component(s) -->
</main>

{{>footer}}
