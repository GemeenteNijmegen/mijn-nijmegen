<main>
    <!-- START: template component(s) -->
    <div class="container" id="section-1">
        <div class="row">
            <div class="col-8">
                <h1>Mijn Nijmegen</h1>
                <p class="lead">Hier vindt u een overzicht van uw uitkeringen.</p>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4">
                <div class="card card-services">
                    <div class="card-body">
                        <p><i class="mdi mdi-account" aria-hidden="true"></i>
                        <h2 class="card-title">Persoonsgegevens</h2>
                        <p class="card-text">Bekijk uw persoons- en adresgegevens.</p>
                        <p class="card-read-more text-right mb-0 text-uppercase">
                            <a href="/persoonsgegevens" aria-label="Bekijk mijn persoonsgegevens">Lees meer</a>
                        </p>
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="card card-services">
                    <div class="card-body">
                        <p><i class="mdi mdi-currency-eur" aria-hidden="true"></i></p>
                        <h2 class="card-title">Uitkeringen</h2>
                        <p class="card-text">Bekijk uw uitkeringsgegevens.</p>
                        <p class="card-read-more text-right mb-0 text-uppercase">
                            <a href="/uitkeringen" aria-label="Bekijk mijn uitkeringen">Lees meer</a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- END: template component(s) -->
</main>

